#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
#define REP(i, a, b) for (int i = a; i >= b; i--)

void solve() {
    string s = "I LOVE CODENECTION";
    ll n;
    
    cin >> n;
    
    if (n % 2) {
        REP(i, s.length() - 1, 0)
            cout << s[i];
    }
    else cout << s;
}

int main() {
    int t = 1;
    while (t--)
        solve();
}
