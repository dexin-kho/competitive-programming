#include <bits/stdc++.h>
using namespace std;

#define REP(i, a, b) for (int i = a; i < b; i++)

int main() {  
    int n, m;
    cin >> n >> m;
    
    char g[n][m];
    REP(i, 0, n)
        REP(j, 0, m)
            cin >> g[i][j];
    
    REP(i, 0, n) {
        REP(j, 0, m) {
            if (g[i][j] == '#') {
                cout << i + 1 << ' ' << j + 1 << endl;
                return 0;
            }
        }
    }
    
    cout << "-1\n";
}
